{
  "name": "amped-react-boilerplate",
  "version": "3.0.0-alpha.2",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "compile:stage": "NODE_ENV=stage npm run compile",
    "start": "better-npm-run start"
  },
  "betterScripts": {
    "compile": {
      "command": "rm -rf ./dist && node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rebelpixeldev/amped-react-boilerplate.git"
  },
  "author": "Ted Sczelecki <ted@rebelpixel.ca> (http://rebelpixel.ca)",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.13",
    "compression": "^1.6.2",
    "css-loader": "^0.26.0",
    "cssnano": "^3.7.4",
    "debug": "^2.2.0",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.7.0",
    "ip": "^1.1.2",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "material-ui": "^0.17.0",
    "moment": "^2.17.1",
    "node-sass": "^4.0.0",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.1.0",
    "prop-types": "^15.5.10",
    "react": "^15.0.0",
    "react-chartjs-2": "^2.1.0",
    "react-dom": "^15.0.0",
    "react-js-pagination": "^2.0.2",
    "react-quill": "^1.0.0-rc.2",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-page-transition": "^3.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "react-transition-group": "^1.1.1",
    "recharts": "^1.0.0-alpha.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "socket.io": "^2.0.1",
    "socket.io-client": "^2.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.14",
    "whatwg-fetch": "^2.0.3",
    "yargs": "^6.3.0"
  },
  "devDependencies": {
    "nodemon": "^1.10.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
